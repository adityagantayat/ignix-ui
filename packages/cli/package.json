{
  "name": "@animation-ui/cli",
  "version": "1.0.0",
  "description": "CLI for installing and managing UI components",
  "main": "dist/index.js",
  "bin": {
    "animation-ui": "./dist/index.js"
  },
  "type": "module",
  "scripts": {
    "build": "tsup src/index.ts --format esm",
    "dev": "tsc -w",
    "lint": "eslint src --ext .ts",
    "test": "jest",
    "clean": "rimraf dist",
    "prepare": "npm run clean && npm run build"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@antfu/ni": "^0.21.8",
    "axios": "^1.7.9",
    "chalk": "^5.3.0",
    "commander": "^11.0.0",
    "fs-extra": "^11.1.1",
    "ora": "^7.0.1",
    "prompts": "^2.4.2",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^20.5.7",
    "@types/prompts": "^2.4.4",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.56.0",
    "rimraf": "^5.0.5",
    "tsup": "^7.3.0",
    "typescript": "^5.2.2"
  }
}
